{
  "name": "ftc-thing",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start-frontend": "cd frontend && npm start",
    "start-backend": "cd backend && npm start",
    "frontend-web": "cd frontend && npm run web",
    "frontend-android": "cd frontend && npm run android",
    "frontend-ios": "cd frontend && npm run ios",
    "dev": "concurrently --kill-others \"npm run dev -w frontend\" \"npm run dev -w backend\"",
    "dev-web": "concurrently \"npm run dev -w backend\" \"npm run web -w frontend\"",
    "dev-android": "concurrently \"npm run dev -w backend\" \"npm run android -w frontend\"",
    "dev-ios": "concurrently \"npm run dev -w backend\" \"npm run ios -w frontend\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "firebase-init": "firebase init",
    "firebase-deploy": "firebase deploy"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "firebase": "^12.9.0",
    "prettier": "^3.7.4",
    "wait-on": "^9.0.3"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs"
}
