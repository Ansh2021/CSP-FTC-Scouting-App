{
  "expo": {
    "name": "CSP FTC Scout",
    "slug": "CSPFTCScout",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/columbusspaceprogram.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons/columbusspaceprogram.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/columbusspaceprogram.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/icons/columbusspaceprogram.png"
    },
    "plugins": ["expo-secure-store", "expo-font"]
  }
}
